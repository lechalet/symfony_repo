{% extends 'base.html.twig' %}

{% block title %}Bon Choix !{% endblock %}

{% block body %}
    <h1>Toutes nos séries : </h1>
    {% for seasons in programs %}
        <div>
            <p>{{ seasons.id }}</p>
        </div>
    {% else %}
        Aucune série trouvée.
    {% endfor %}
    <a href="{{ path('show_episode') }}">
        Retour à l'accueil
    </a>
{% endblock %}